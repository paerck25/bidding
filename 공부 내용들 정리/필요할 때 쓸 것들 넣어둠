
// 도커 레디스 실행 
docker run --name redis -d -p 6379:6379 redis

@Repository
@Transactional
public interface InventoryRepository extends PagingAndSortingRepository<Inventory, Long> {

  List<Inventory> findByIdIn(List<Long> ids); // Serialized 하면 이렇게 쓰기 가능

}

